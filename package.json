{
  "name": "uuidjs",
  "version": "3.5.3",
  "title": "UUID.js",
  "description": "RFC-compliant UUID Generator for JavaScript",
  "main": "src/uuid.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "tonicExampleFilename": "example.js",
  "scripts": {
    "test": "qunit test/test.nodejs.js",
    "jsdoc": "jsdoc -R ./README.md -d ./doc src/uuid.js",
    "types": "jsdoc -t node_modules/tsd-jsdoc -d ./typings src/uuid.core.js && node typings/replace.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/LiosK/UUID.js.git"
  },
  "keywords": [
    "library",
    "uuid",
    "guid",
    "rfc",
    "4122",
    "universally",
    "globally",
    "unique",
    "identifier"
  ],
  "author": "LiosK <contact@mail.liosk.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LiosK/UUID.js/issues"
  },
  "homepage": "https://github.com/LiosK/UUID.js",
  "devDependencies": {
    "jsdoc": "^3.4.3",
    "qunitjs": "^1.23.1",
    "tsd-jsdoc": "^1.0.1"
  },
  "types": "typings/types.d.ts"
}
